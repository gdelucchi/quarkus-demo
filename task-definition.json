{
  "family": "quarkus",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE", "EC2"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "quarkus-app",
      "image": "ualter/quarkus-app:latest",
      "memoryReservation": 128,
      "cpu": 512,
      "memory": 1024,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "command": [
        "./application", "-Dquarkus.http.host=0.0.0.0"
      ],
      "essential": true
    }
  ],
  "awsvpcConfiguration": {
    "subnets": ["subnet-df7c3f94", "subnet-7996b000", "subnet-144e7e4e"],
    "securityGroups": ["sg-0507cb12a7638f8b4"],
    "assignPublicIp": "ENABLED"
  }
}
